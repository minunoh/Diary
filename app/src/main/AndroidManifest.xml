<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.koreatech.diary">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Diary">
        <activity
            android:name="com.koreatech.diary.SignUpActivity"
            android:exported="true" />
        <activity
            android:name="com.koreatech.diary.Calendar"
            android:exported="true" />
        <activity
            android:name="com.koreatech.diary.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.koreatech.diary.MainActivity"
            android:exported="true" />
        <activity android:name=".WDiaryActivity"
            android:exported="true"/>
        <activity android:name=".WScheduleActivity"
            android:exported="true"/>
        <activity android:name=".MydiaryActivity"
            android:exported="true"/>
        <activity
            android:name=".CommentActivity"
            android:label="Comment"/>
        <activity
            android:name=".FeedActivity"
            android:label="Comment"/>
    </application>

</manifest>